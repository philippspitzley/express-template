{
  "name": "express-template",
  "version": "1.0.0",
  "description": "Starter for express server with drizzle orm",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "start": "node --import tsx src/index.ts",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "db:seed": "tsx src/db/seed.ts",
    "dc:up": "docker compose -p express-dev --env-file .env up -d",
    "dc:down": "docker compose -p express-dev --env-file .env down",
    "dc:up:test": "docker compose -p express-test --env-file .env.test up -d",
    "dc:down:test": "docker compose -p express-test --env-file .env.test down -v",
    "neondb:dev": "npx get-db -y -k NEON_DATABASE_URL",
    "neondb:test": "npx get-db -y -k NEON_DATABASE_URL -e ./.env.test",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "simple-git-hooks": {
    "pre-commit": "npx nano-staged"
  },
  "nano-staged": {
    "*.{js,jsx,ts,tsx,json,md,yml,yaml}": [
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "keywords": [
    "template",
    "api",
    "nodejs",
    "express",
    "drizzle"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@epic-web/remember": "^1.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.44.4",
    "drizzle-zod": "^0.8.3",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "morgan": "^1.10.1",
    "postgres": "^3.4.7",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.10",
    "@types/node": "^20.11.5",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9.39.2",
    "globals": "^17.0.0",
    "nano-staged": "^0.9.0",
    "prettier-plugin-organize-imports": "^4.3.0",
    "simple-git-hooks": "^2.13.1",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.53.1",
    "vitest": "^3.2.4"
  }
}
